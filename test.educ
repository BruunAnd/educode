program c
  method main()
    robot.say("out " + random(1, 20))
    number num = 0
    repeat while num less than 50
      Coordinates coords = robot.getCoordinates()
      robot.setWorldTime(1200)
      coords.setX(coords.getX() + random(-2, 15))
      robot.walkTo(coords)
      robot.wait(5000)
      num = num + 1
    end repeat
  end method
end program